<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:parentTag="androidx.constraintlayout.widget.ConstraintLayout">



    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_small_rank"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/shape_circle_image_silver"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <TextView
        android:id="@+id/tv_second_rank"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="@drawable/shape_circle_rating_silver"
        android:fontFamily="@font/gilroy_medium"
        android:gravity="center"
        android:text="@string/text_rating_first_example"
        android:textColor="#F5F5F5"
        android:textSize="14sp"
        app:layout_constraintTop_toBottomOf="@id/img_small_rank"
        app:layout_constraintBottom_toBottomOf="@id/img_small_rank"
        app:layout_constraintStart_toStartOf="@id/img_small_rank"
        app:layout_constraintEnd_toEndOf="@id/img_small_rank"
        tools:ignore="TextContrastCheck,TextContrastCheck,TextContrastCheck"/>


</merge>
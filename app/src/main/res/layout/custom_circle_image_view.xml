<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:parentTag="androidx.constraintlayout.widget.ConstraintLayout">



    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_profile_first_rank"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:background="@drawable/shape_circle_image_gold"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <TextView
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:background="@drawable/shape_circle_rating_gold"
        android:fontFamily="@font/gilroy_medium"
        android:gravity="center"
        android:text="@string/text_rating_first_example"
        android:textColor="#F5F5F5"
        android:textSize="16sp"
        app:layout_constraintTop_toBottomOf="@id/img_profile_first_rank"
        app:layout_constraintBottom_toBottomOf="@id/img_profile_first_rank"
        app:layout_constraintStart_toStartOf="@id/img_profile_first_rank"
        app:layout_constraintEnd_toEndOf="@id/img_profile_first_rank"
        tools:ignore="TextContrastCheck,TextContrastCheck,TextContrastCheck"/>


</merge>